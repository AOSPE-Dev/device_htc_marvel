#
# Configuration for HTC Wildfire S [Marvel]
#


# RIL/GSM
#
ro.phone.min_match.HTC__016=7
ro.ril.ecc.HTC-GCC=999,112,997
ro.ril.ecc.HTC-WWE=999
ro.ril.ecc.HTC-ITA=113,115,118
ro.ril.enable.a52.HTC-ITA=1
ro.ril.enable.a53.HTC-ITA=1
ro.ril.enable.a52=0
ro.ril.enable.a53=1
ro.ril.disable.fd.plmn.prefix=23402,23410,23411,23420
ro.ril.enable.sdr=1
ro.ril.enable.amr.wideband=0
ro.ril.hep = 1
rild.libpath=/system/lib/libhtc_ril.so
ro.telephony.default_network=0
ro.ril.enable.dtm=1
ro.ril.hsdpa.category=8
ro.ril.hsupa.category=5
ro.ril.def.agps.mode=2
ro.ril.hsxpa=2
ro.ril.gprsclass=12
ro.telephony.ril.v3=signalstrength,skipbrokendatacall
# For HSDPA low throughput
ro.ril.disable.power.collapse = 1

# For PDP overlap problem
ro.ril.avoid.pdp.overlap = 1

# update the acoustic table for Buzz NEL lab testing
ro.ril.update.acoustic.table = 1

# Modify MMS APN retry timer from 5s to 2s.
ro.gsm.2nd_data_retry_config = max_retries=3, 2000, 2000, 2000
mobiledata.interfaces=rmnet0,rmnet1,rmnet2,gprs,ppp0

# WI-FI
wifi.interface=wlan0
wifi.supplicant_scan_interval=150


# Display
ro.sf.lcd_density=160
ro.sf.hw_lcd_density=150
video.accelerate.hw=1
ro.hwaccel.lockscreen=0
view.scroll_friction=0.008
view.minimum_fling_velocity=25

# System
debug.sf.no_hw_vsync=1
debug.composition.type=gpu
persist.sys.use_dithering=0
persist.sys.purgeable_assets=1
#debug.sf.hw=1
#debug.gr.numframebuffers=3
#com.qc.hdmi_out=false
ro.compcache.default=26
sys.checkfs.fat=false
persist.sys.use_dithering=2

#settings.display.autobacklight=1
#settings.display.brightness=88
#persist.service.mount.playsnd=0
#ro.setupwizard.mode=OPTIONAL
#ro.config.sync=yes
#net.bt.name=Android
ro.com.google.locationfeatures=0
ro.setupwizard.enable_bypass=1

# Audio/Video
ro.media.dec.aud.wma.enabled=1
ro.media.dec.vid.wmv.enabled=1
ro.media.dec.jpeg.memcap=20000000
debug.camcorder.disablemeta=1

# HwaSettings
hwui.use.blacklist=true


# Dalvik
dalvik.vm.lockprof.threshold=500
dalvik.vm.dexopt-flags=m=y
dalvik.vm.heapstartsize=5m
dalvik.vm.heapgrowthlimit=24m
dalvik.vm.heapsize=24m
dalvik.vm.verify_bytecode=false
dalvik.vm.execution-mode=int:fast
dalvik.vm.checkjni=0
dalvik.vm.dexopt-data-only=1

ro.owl.version=Release-Candidate-4
ro.beatsaudio.version=Google Play Edition One


# USB
persist.sys.usb.config=mass_storage,mtp,adb
persist.service.adb.enable=1

ro.product.camera=marvel
